<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://java.sun.com/xml/ns/javaee" xmlns:web="http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd" xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_3_0.xsd" id="WebApp_ID" version="3.0">
  <display-name>irt</display-name>
  <welcome-file-list>
    <welcome-file>index.html</welcome-file>
    <welcome-file>index.htm</welcome-file>
    <welcome-file>index.jsp</welcome-file>
    <welcome-file>default.html</welcome-file>
    <welcome-file>default.htm</welcome-file>
    <welcome-file>default.jsp</welcome-file>
  </welcome-file-list>
  <servlet>
    <servlet-name>home</servlet-name>
    <servlet-class>irt.web.HomeServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>home</servlet-name>
    <url-pattern>/index.html</url-pattern>
  </servlet-mapping>
  <servlet>
  	<servlet-name>login</servlet-name>
  	<servlet-class>irt.web.LoginServlet</servlet-class>
  </servlet>
  <servlet-mapping>
  	<servlet-name>login</servlet-name>
  	<url-pattern>/login</url-pattern>
  </servlet-mapping>
  <servlet>
  	<servlet-name>history</servlet-name>
  	<servlet-class>irt.web.HistoryServlet</servlet-class>
  </servlet>
  <servlet-mapping>
  	<servlet-name>history</servlet-name>
  	<url-pattern>/history</url-pattern>
  </servlet-mapping>
  <servlet>
  	<servlet-name>part-number</servlet-name>
  	<servlet-class>irt.web.PartNumberServlet</servlet-class>
  </servlet>
  <servlet-mapping>
  	<servlet-name>part-number</servlet-name>
  	<url-pattern>/part-numbers</url-pattern>
  </servlet-mapping>
   <servlet>
      <servlet-name>users</servlet-name>
      <servlet-class>irt.web.UsersServlet</servlet-class>
   </servlet>
   <servlet-mapping>
      <servlet-name>users</servlet-name>
      <url-pattern>/users</url-pattern>
   </servlet-mapping>
   <servlet>
      <servlet-name>sellers</servlet-name>
      <servlet-class>irt.web.SellersServlet</servlet-class>
   </servlet>
   <servlet-mapping>
      <servlet-name>sellers</servlet-name>
      <url-pattern>/sellers</url-pattern>
   </servlet-mapping>
   <servlet>
      <servlet-name>purchase</servlet-name>
      <servlet-class>irt.web.PurchaseServlet</servlet-class>
   </servlet>
   <servlet-mapping>
      <servlet-name>purchase</servlet-name>
      <url-pattern>/purchase</url-pattern>
   </servlet-mapping>
   <servlet>
      <servlet-name>movement</servlet-name>
      <servlet-class>irt.web.ComponentsMovementServlet</servlet-class>
   </servlet>
   <servlet-mapping>
      <servlet-name>movement</servlet-name>
      <url-pattern>/components_movement</url-pattern>
   </servlet-mapping>
   <servlet>
      <servlet-name>product</servlet-name>
      <servlet-class>irt.web.ProductServlet</servlet-class>
   </servlet>
   <servlet-mapping>
      <servlet-name>product</servlet-name>
      <url-pattern>/product_structure</url-pattern>
   </servlet-mapping>
  <servlet>
      <servlet-name>add-link</servlet-name>
      <servlet-class>irt.web.AddLinkServlet</servlet-class>
   </servlet>
   <servlet-mapping>
      <servlet-name>add-link</servlet-name>
      <url-pattern>/add-link</url-pattern>
   </servlet-mapping>
  <servlet>
      <servlet-name>alt-mfr-pns</servlet-name>
      <servlet-class>irt.web.AlternativePNServlet</servlet-class>
   </servlet>
   <servlet-mapping>
      <servlet-name>alt-mfr-pns</servlet-name>
      <url-pattern>/alt-mfr-pns</url-pattern>
   </servlet-mapping>
     <servlet>
      <servlet-name>cost</servlet-name>
      <servlet-class>irt.web.CostServlet</servlet-class>
   </servlet>
   <servlet-mapping>
      <servlet-name>cost</servlet-name>
      <url-pattern>/cost</url-pattern>
   </servlet-mapping>
  <servlet>
      <servlet-name>manufacture-links</servlet-name>
      <servlet-class>irt.web.ManufactureServlet</servlet-class>
   </servlet>
   <servlet-mapping>
      <servlet-name>manufacture-links</servlet-name>
      <url-pattern>/manufacture-links</url-pattern>
   </servlet-mapping>

  <listener>
   <listener-class>irt.web.Init</listener-class>
</listener>
</web-app>